{
  "buildCommand": "",
  "framework": null,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "/api/index.py" },
    { "src": "/solution/(.*)", "dest": "/api/index.py" },
    { "src": "/admin(.*)", "dest": "/api/index.py" }
  ]
}
